      type: 0
      typeString: CustomFieldType_Text
  - id: 22
    fields:
    - title: Name
      value: "Pella\u2019s Wish (D2)"
      type: 0
      typeString: CustomFieldType_Text
    - title: Description
      value: "Outpost between The Imperium Vitale and Ath\u2019rak. Responsible for
        traffic between Ath\u2019rak, the northern Ashborne Tower, and back. They
        have limited borders, but still less restrictive than that of Zodia."
      type: 0
      typeString: CustomFieldType_Text
